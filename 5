#include <iostream>
#include <cmath>
using namespace std;
int main()
{
	int i, N, u = 0, j, p = -1, pmax = -1;
	int xmax[3] = { 0 };
	int ymax[3] = { 0 };
	cout << "Enter the amount of points: ";
	cin >> N;
	int* xarray = new int[N];
	int* yarray = new int[N];
	cout << "Enter x: ";
	for (i = 0; i < N; i++) {
		cin >> xarray[i];
	}
	cout << "Enter y: ";
	for (i = 0; i < N; i++) {
		cin >> yarray[i];
	}
	for (i = 0; i < N; i++)
	{
		 for (j=0; j<N; j++)
			 for (u = 0; u < N; u++) {

				 p = sqrt(powf((xarray[i] - xarray[j]), 2) + powf((yarray[i] - yarray[j]), 2)) + sqrt(powf((xarray[i] - xarray[u]), 2) + powf((yarray[i] - yarray[u]), 2)) + sqrt(powf((xarray[j] - xarray[u]), 2) + powf((yarray[j] - yarray[u]), 2));
				 if (p > pmax) {
					 pmax = p;
					 xmax[0] = xarray[i];
					 xmax[1] = xarray[j];
					 xmax[2] = xarray[u];
					 ymax[0] = yarray[i];
					 ymax[1] = yarray[j];
					 ymax[2] = yarray[u];
				 }
			 }
	}
	for (i=0; i<3; i++) cout << "(" << xmax[i] << "," << ymax[i] << ")"<< "\n";

}
