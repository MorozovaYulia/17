#include <iostream>
#include <math.h>
using namespace std;
int main()
{
	int i, N, i1 = 0, h = 0, r = -1, rmax = -1, xmax = 1, ymax = -1;
	cout << "Enter the amount of points: ";
	cin >> N;
	int* xarray = new int[N];
	int* yarray = new int[N];
	cout << "Enter x: ";
	for (i = 0; i < N; i++) {
		cin >> xarray[i];
	}
	cout << "Enter y: ";
	for (i = 0; i < N; i++) {
		cin >> yarray[i];
	}
	for (i = 0; i < N; i++)
	{
		
			if (xarray[i] < 0 && yarray[i]>0) {
				r = sqrt(xarray[i] * xarray[i] + yarray[i] * yarray[i]);
				if (r > rmax) {
					rmax = r;
					xmax = xarray[i];
					ymax = yarray[i];
				}
			}
	}
	
	if (rmax == -1) cout << "(0,0)";
	else cout << "(" << xmax << "," << ymax << ")";

}
